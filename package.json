{
  "name": "@lastui/rocker",
  "version": "0.21.8",
  "license": "Apache-2.0",
  "author": "jan.cajthaml@gmail.com",
  "homepage": "https://github.com/lastui/rocker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lastui/rocker.git"
  },
  "bugs": {
    "url": "https://github.com/lastui/rocker/issues"
  },
  "bin": {
    "rocker": "cli/index.js"
  },
  "files": [
    "babel/*",
    "bootstrap/dll/*-manifest.json",
    "bootstrap/dll/*.js",
    "bootstrap/index.js",
    "cli/*",
    "jest/*",
    "platform/dll/*-manifest.json",
    "platform/dll/*.js",
    "platform/index.js",
    "webpack/*"
  ],
  "scripts": {
    "build": "npm run build:dependencies && npm run build:platform && npm run build:bootstrap",
    "postbuild": "node --no-deprecation ./scripts/verify-dlls.mjs && node ./cli/index.js lint --fix",
    "build:bootstrap": "npm run build:bootstrap:dev && npm run build:bootstrap:prod",
    "prebuild:bootstrap:dev": "node --no-deprecation ./bootstrap/scripts/link.mjs",
    "build:bootstrap:dev": "node --no-deprecation  ./cli/index.js build --cwd=bootstrap --dev",
    "postbuild:bootstrap:dev": "node --no-deprecation ./bootstrap/scripts/unlink.mjs",
    "prebuild:bootstrap:prod": "node --no-deprecation ./bootstrap/scripts/link.mjs",
    "build:bootstrap:prod": "node --no-deprecation ./cli/index.js build --cwd=bootstrap",
    "postbuild:bootstrap:prod": "node --no-deprecation ./bootstrap/scripts/unlink.mjs",
    "build:dependencies": "npm --prefix=dependencies run build",
    "build:platform": "npm run build:platform:dev && npm run build:platform:prod",
    "prebuild:platform:dev": "node --no-deprecation ./platform/scripts/link.mjs",
    "build:platform:dev": "node --no-deprecation ./cli/index.js build --cwd=platform --dev",
    "postbuild:platform:dev": "node --no-deprecation ./platform/scripts/unlink.mjs",
    "prebuild:platform:prod": "node --no-deprecation ./platform/scripts/link.mjs",
    "build:platform:prod": "node --no-deprecation ./cli/index.js build --cwd=platform",
    "postbuild:platform:prod": "node --no-deprecation ./platform/scripts/unlink.mjs",
    "lint": "node --no-deprecation ./cli/index.js lint",
    "test": "npm run test:platform && npm run test:bootstrap",
    "test:bootstrap": "node --no-deprecation ./cli/index.js test --cwd=bootstrap",
    "test:platform": "node --no-deprecation ./cli/index.js test --cwd=platform"
  },
  "dependencies": {
    "@babel/core": "7.28.5",
    "@babel/eslint-parser": "7.28.5",
    "@babel/parser": "7.28.5",
    "@babel/plugin-transform-modules-commonjs": "7.27.1",
    "@babel/preset-env": "7.28.5",
    "@babel/preset-react": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@babel/register": "7.28.3",
    "@dotenvx/dotenvx": "1.51.1",
    "@linaria/core": "6.3.0",
    "@linaria/postcss-linaria": "6.3.0",
    "@linaria/react": "6.3.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@wyw-in-js/webpack-loader": "0.8.0",
    "ansi-colors": "4.1.3",
    "babel-loader": "10.0.0",
    "buffer": "6.0.3",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.2",
    "eslint": "9.39.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react": "7.37.5",
    "glob": "11.0.3",
    "globals": "16.5.0",
    "html-webpack-plugin": "5.6.4",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "json5-loader": "4.0.1",
    "mini-css-extract-plugin": "2.9.4",
    "npm-registry-fetch": "19.1.1",
    "path-browserify": "1.0.1",
    "prettier": "3.6.2",
    "prettier-package-json": "2.8.0",
    "pretty-format": "30.2.0",
    "redux-mock-store": "1.5.5",
    "source-map": "github:mozilla/source-map#master",
    "style-loader": "4.0.0",
    "stylelint": "16.25.0",
    "stylelint-config-standard": "39.0.1",
    "terser-webpack-plugin": "5.3.14",
    "util": "0.12.5",
    "weak-napi": "2.0.2",
    "webpack": "5.102.1",
    "webpack-dev-server": "5.2.2",
    "webpack-merge": "6.0.1",
    "whatwg-fetch": "3.6.20",
    "yargs": "18.0.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
