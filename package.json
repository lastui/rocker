{
  "name": "@lastui/rocker",
  "version": "0.12.10",
  "scripts": {
    "lint": "node ./cli/index.js lint",
    "build": "npm run build:dependencies && npm run build:platform && npm run build:bootstrap",
    "prebuild:dependencies": "cd dependencies && npm ci --no-fund --no-audit",
    "build:dependencies": "npm run build:dependencies:dev && npm run build:dependencies:prod",
    "build:dependencies:prod": "cd dependencies && node ../cli/index.js build",
    "build:dependencies:dev": "cd dependencies && node ../cli/index.js build --dev",
    "prebuild:platform": "cd platform && node ./scripts/prebuild.js",
    "build:platform": "npm run build:platform:dev && npm run build:platform:prod",
    "build:platform:prod": "cd platform && node ../cli/index.js build",
    "build:platform:dev": "cd platform && node ../cli/index.js build --dev",
    "postbuild:platform": "cd platform && node ./scripts/postbuild.js",
    "prebuild:bootstrap": "cd bootstrap && node ./scripts/prebuild.js",
    "build:bootstrap": "npm run build:bootstrap:dev && npm run build:bootstrap:prod",
    "build:bootstrap:prod": "cd bootstrap && node ../cli/index.js build",
    "build:bootstrap:dev": "cd bootstrap && node ../cli/index.js build --dev",
    "postbuild:bootstrap": "cd bootstrap && node ./scripts/postbuild.js"
  },
  "bin": {
    "rocker": "./cli/index.js"
  },
