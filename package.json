{
  "name": "@lastui/rocker",
  "version": "0.20.18",
  "license": "Apache-2.0",
  "author": "jan.cajthaml@gmail.com",
  "homepage": "https://github.com/lastui/rocker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lastui/rocker.git"
  },
  "bugs": {
    "url": "https://github.com/lastui/rocker/issues"
  },
  "bin": {
    "rocker": "cli/index.js"
  },
  "files": [
    "babel/*",
    "bootstrap/dll/*-manifest.json",
    "bootstrap/dll/*.js",
    "bootstrap/index.js",
    "cli/*",
    "jest/*",
    "platform/dll/*-manifest.json",
    "platform/dll/*.js",
    "platform/index.js",
    "webpack/*"
  ],
  "scripts": {
    "build": "npm run build:dependencies && npm run build:platform && npm run build:bootstrap",
    "postbuild": "node ./scripts/verify-dlls.mjs && node ./cli/index.js lint --fix",
    "build:bootstrap": "npm run build:bootstrap:dev && npm run build:bootstrap:prod",
    "prebuild:bootstrap:dev": "node ./bootstrap/scripts/link.mjs",
    "build:bootstrap:dev": "node ./cli/index.js build --cwd=bootstrap --dev",
    "postbuild:bootstrap:dev": "node ./bootstrap/scripts/unlink.mjs",
    "prebuild:bootstrap:prod": "node ./bootstrap/scripts/link.mjs",
    "build:bootstrap:prod": "node ./cli/index.js build --cwd=bootstrap",
    "postbuild:bootstrap:prod": "node ./bootstrap/scripts/unlink.mjs",
    "build:dependencies": "npm --prefix=dependencies run build",
    "build:platform": "npm run build:platform:dev && npm run build:platform:prod",
    "prebuild:platform:dev": "node ./platform/scripts/link.mjs",
    "build:platform:dev": "node ./cli/index.js build --cwd=platform --dev",
    "postbuild:platform:dev": "node ./platform/scripts/unlink.mjs",
    "prebuild:platform:prod": "node ./platform/scripts/link.mjs",
    "build:platform:prod": "node ./cli/index.js build --cwd=platform",
    "postbuild:platform:prod": "node ./platform/scripts/unlink.mjs",
    "lint": "node ./cli/index.js lint",
    "test": "npm run test:platform && npm run test:bootstrap",
    "test:bootstrap": "node ./cli/index.js test --cwd=bootstrap",
    "test:platform": "node ./cli/index.js test --cwd=platform"
  },
  "dependencies": {
    "@babel/core": "7.24.6",
    "@babel/eslint-parser": "7.24.6",
    "@babel/parser": "7.24.6",
    "@babel/plugin-proposal-export-default-from": "7.24.6",
    "@babel/plugin-proposal-throw-expressions": "7.24.6",
    "@babel/plugin-transform-modules-commonjs": "7.24.6",
    "@babel/preset-env": "7.24.6",
    "@babel/preset-react": "7.24.6",
    "@babel/preset-typescript": "7.24.6",
    "@babel/register": "7.24.6",
    "@linaria/core": "6.2.0",
    "@linaria/react": "6.2.1",
    "@linaria/stylelint-config-standard-linaria": "6.2.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@wyw-in-js/webpack-loader": "0.5.4",
    "add-asset-html-webpack-plugin": "6.0.0",
    "ansi-colors": "4.1.3",
    "babel-loader": "9.1.3",
    "buffer": "6.0.3",
    "copy-webpack-plugin": "12.0.2",
    "core-js": "3.37.1",
    "css-loader": "7.1.2",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-react": "7.34.1",
    "html-webpack-plugin": "5.6.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "json5-loader": "4.0.1",
    "mini-css-extract-plugin": "2.9.0",
    "npm-registry-fetch": "17.1.0",
    "path-browserify": "1.0.1",
    "prettier": "3.3.3",
    "prettier-package-json": "2.8.0",
    "pretty-format": "29.7.0",
    "redux-mock-store": "1.5.4",
    "source-map": "github:mozilla/source-map#master",
    "style-loader": "4.0.0",
    "stylelint": "16.8.1",
    "terser-webpack-plugin": "5.3.10",
    "util": "0.12.5",
    "weak-napi": "2.0.2",
    "webpack": "5.93.0",
    "webpack-dev-server": "5.0.4",
    "webpack-stats-plugin": "1.1.3",
    "whatwg-fetch": "3.6.20",
    "yargs": "17.7.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
